---
apiVersion: v1
kind: Secret
metadata:
  name: cluster-memos-secrets
  namespace: default
stringData:
  MEMOS_DSN: "postgresql://{{ memos_db_user }}:{{ memos_db_password }}@postgres-lb.storage:5432/memos?sslmode=disable"
  POSTGRES_USER: "{{ memos_db_user }}"
  POSTGRES_PASSWORD: "{{ memos_db_password }}"
  MEMOS_S3_ACCESS_KEY: "{{ memos_s3_access_key }}"
  MEMOS_S3_SECRET_KEY: "{{ memos_s3_secret_key }}"